{
  "name": "@metamask-institutional/adapters-cli",
  "version": "0.14.0",
  "description": "Local CLI for Defi adapters",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:watch": "npm run build -- --watch",
    "test": "vitest src",
    "test:file": "if [ -z \"$TEST_FILE\" ]; then echo 'No test file specified'; exit 1; else vitest $TEST_FILE; fi",
    "test:tsc": "tsc --noEmit -p tsconfig.json",
    "test:cov": "vitest src --ci --coverage  --coverageDirectory='coverage/unit/'",
    "test:update:adapter-snapshot": "vitest src/tests/new-adapter-templates.test.ts -u"
  },
  "devDependencies": {
    "@metamask-institutional/defi-adapters": "*",
    "@metamask-institutional/workers": "*",
    "@solana/web3.js": "^1.98.0",
    "@types/better-sqlite3": "^7.6.11",
    "@types/ethereum-block-by-date": "^1.4.1",
    "@types/inquirer": "^8.2.6",
    "@types/jest": "^29.5.12",
    "@types/lodash-es": "^4.17.12",
    "better-sqlite3": "^11.2.1",
    "chalk": "^5.4.1",
    "commander": "^11.0.0",
    "ethereum-block-by-date": "^1.4.9",
    "ethers": "^6.7.1",
    "inquirer": "^12.4.2",
    "jest": "^29.7.0",
    "lodash-es": "^4.17.21",
    "msw": "^2.3.4",
    "npm-run-all": "^4.1.5",
    "p-queue": "^8.1.0",
    "recast": "^0.23.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "zod": "^3.22.4"
  }
}
